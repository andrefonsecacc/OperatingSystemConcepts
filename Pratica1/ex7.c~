#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc,char *argv[]){

  FILE *fp;
  int c;
  char m, n , rest;
  int row, columns;
    
  fp = fopen(argv[1],"r");
  while(1)
    {
      c = fgetc(fp);
      if( feof(fp) )
	{ 
	  break ;
	}
      // printf("%c", c);
      //row = atoi(m);
      // columns=atoi(n);
      printf("%d",c);
    }
  fclose(fp);
  return 0;
 
}
